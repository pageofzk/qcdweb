    <div class="worth_number"><i class="icons icon_logo42"><img src=""></i><b><%- message %></b></div>
    <div class="comments">
        <!-- 多说评论框 start -->
        <div class="ds-thread" data-thread-key="feedback" data-title="<%- title %>" data-url="qicaidang.cn/feadback/"></div>
        <!-- 多说评论框 end -->
        <!-- 多说公共JS代码 start (一个网页只需插入一次) -->
        <script type="text/javascript">
            var duoshuoQuery = {short_name:"qicaidang"};
            (function() {
                var ds = document.createElement('script');
                ds.type = 'text/javascript';ds.async = true;
                ds.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') + '//static.duoshuo.com/embed.js';
                ds.charset = 'UTF-8';
                (document.getElementsByTagName('head')[0]
                || document.getElementsByTagName('body')[0]).appendChild(ds);
            })();
        </script>
        <!-- 多说公共JS代码 end -->
    </div>

</article>
